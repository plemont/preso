{
  "name": "Preso",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Refresh'n'loop presentations",
  "homepage_url": "https://plemont.github.io",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_title": "Present'n'loop"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "activeTab"
  ]
}
